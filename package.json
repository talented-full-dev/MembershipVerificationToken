{
  "name": "electusprotocol",
  "version": "1.0.6",
  "description": "Standard Implementation and interface for Electus Protocol",
  "main": "truffle.js",
  "directories": {
    "test": "test"
  },
  "scripts": {
    "start:blockchain:client": "bash ./scripts/start-blockchain.sh",
    "stop:blockchain:client": "bash ./scripts/stop-blockchain.sh",
    "test": "npm run start:blockchain:client & truffle migrate --reset --compile-all && truffle test --network development",
    "post-test": "npm run stop:blockchain:client",
    "coverage": "touch allFiredEvents & ./node_modules/.bin/solidity-coverage"
  },
  "files": [
    "contracts",
    "test"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/chaitanyapotti/ElectusProtocol.git"
  },
  "author": "Chaitanya Potti",
  "bugs": {
    "url": "https://github.com/chaitanyapotti/ElectusProtocol/issues"
  },
  "keywords": [
    "solidity",
    "ethereum",
    "smart",
    "contracts",
    "protocol",
    "electus",
    "eip-1261"
  ],
  "license": "MIT",
  "homepage": "https://github.com/chaitanyapotti/ElectusProtocol#readme",
  "dependencies": {
    "ganache-cli": "^6.1.8",
    "openzeppelin-solidity": "^1.12.0"
  },
  "devDependencies": {
    "coveralls": "^3.0.2",
    "solidity-coverage": "^0.5.11"
  }
}
